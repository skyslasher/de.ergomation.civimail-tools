  <maintainer>
    <author>Andreas Hahn</author>
    <email>andreas@ergomation.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/skyslasher/de.ergomation.civimail-tools</url>
    <url desc="Documentation">https://github.com/skyslasher/de.ergomation.civimail-tools</url>
    <url desc="Support">https://github.com/skyslasher/de.ergomation.civimail-tools</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-11-19</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Not for productive use</comments>
  <civix>
    <namespace>CRM/CivimailTools</namespace>
  </civix>
</extension>
