<?xml version="1.0"?>
<extension key="de.ergomation.civimail-tools" type="module">
  <file>civimail_tools</file>
  <name>Single-Click-Opt-Out for CiviMail</name>
  <description>By enabling this plugin, users that click on the opt-out link will be opted out immediately, without having to type their e-mail adress into the opt-out form.</description>
